{
  "manifest_version": 2,

  "name": "html header editor",
  "description": "",
  "version": "0.0.1",
  "background": {
    "scripts": ["/src/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/src/content.js"],
      "all_frames": false
    }
  ],
  "browser_action": { "default_popup": "/src/popup.html" },
  "permissions": [
          "webRequest",
          "webRequestBlocking",
          "<all_urls>",
          "tabs"
        ]
}
